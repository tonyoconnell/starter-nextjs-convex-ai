{
  "name": "@appydave/log-ingestion-worker",
  "version": "1.0.0",
  "description": "Cloudflare Worker for AI-first logging system with Redis backend",
  "main": "src/index.ts",
  "scripts": {
    "dev": "wrangler dev",
    "deploy": "wrangler deploy --env production",
    "deploy:dev": "wrangler deploy --env development",
    "deploy:production": "wrangler deploy --env production",
    "build": "esbuild src/index.ts --bundle --format=esm --target=es2022 --outfile=dist/index.js",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write src/**/*.ts",
    "setup:secrets": "./scripts/setup-secrets.sh",
    "secrets:list": "npx wrangler secret:list",
    "secrets:list:dev": "npx wrangler secret:list --env development",
    "secrets:list:prod": "npx wrangler secret:list --env production"
  },
  "dependencies": {
    "@cloudflare/workers-types": "^4.20250803.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "esbuild": "^0.19.12",
    "eslint": "^8.57.1",
    "jest": "^29.7.0",
    "prettier": "^3.6.2",
    "typescript": "^5.9.2",
    "wrangler": "^4.27.0"
  },
  "keywords": [
    "cloudflare-workers",
    "logging",
    "redis",
    "ai-development",
    "rate-limiting",
    "monitoring"
  ],
  "author": "AppyDave Templates",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/appydave-templates/starter-nextjs-convex-ai.git",
    "directory": "apps/workers/log-ingestion"
  }
}
